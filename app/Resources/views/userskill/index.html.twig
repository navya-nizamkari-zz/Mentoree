{% extends 'base.html.twig' %}

{% block body %}
    <h1>UserSkill list</h1>

    <table>
        <thead>
            <tr>
                <th>Id</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for userSkill in userSkills %}
            <tr>
                <td><a href="{{ path('userskill_show', { 'id': userSkill.id }) }}">{{ userSkill.id }}</a></td>
                <td>
                    <ul>
                        <li>
                            <a href="{{ path('userskill_show', { 'id': userSkill.id }) }}">show</a>
                        </li>
                        <li>
                            <a href="{{ path('userskill_edit', { 'id': userSkill.id }) }}">edit</a>
                        </li>
                    </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('userskill_new') }}">Create a new entry</a>
        </li>
    </ul>
{% endblock %}
